<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.redes.japo.updmulticast.GameControl">

    <TextView
        android:id="@+id/scoreUser2_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="@android:color/holo_orange_light"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView11"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="@+id/textView11"
        app:layout_constraintHorizontal_bias="0.5" />

    <TextView
        android:id="@+id/scoreUser3_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="@android:color/holo_orange_light"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@+id/textView12"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="@+id/textView12"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView12"
        app:layout_constraintHorizontal_bias="0.5" />

    <TextView
        android:id="@+id/scoreUser1_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="@android:color/holo_orange_light"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView10"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="@+id/textView10"
        app:layout_constraintHorizontal_bias="0.5" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Score"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton4"
        android:id="@+id/textView12"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="@+id/imageButton4"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageButton4"
        app:layout_constraintHorizontal_bias="0.504" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Score"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton3"
        android:id="@+id/textView10"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageButton3"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="@+id/imageButton3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Score"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton2"
        android:id="@+id/textView11"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageButton2"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="@+id/imageButton2" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toTopOf="@+id/user3_txt"
        app:srcCompat="@drawable/duckbutton"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/user3_txt"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="@+id/user3_txt" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toTopOf="@+id/user1_txt"
        app:srcCompat="@drawable/duckbutton"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/user1_txt"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="@+id/user1_txt" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toTopOf="@+id/user2_txt"
        app:srcCompat="@drawable/duckbutton"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/user2_txt"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="@+id/user2_txt" />

    <TextView
        android:id="@+id/user1_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="25dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/user2_txt"
        android:layout_marginRight="120dp" />

    <TextView
        android:id="@+id/user2_txt"
        android:layout_width="58dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="275dp"
        android:layout_marginTop="25dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.0" />

    <TextView
        android:id="@+id/user3_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/user2_txt"
        android:layout_marginLeft="120dp" />

</android.support.constraint.ConstraintLayout>
